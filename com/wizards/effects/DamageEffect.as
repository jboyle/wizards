package com.wizards.effects{	import com.wizards.WizardsG;	public class DamageEffect extends Effect	{		private var _dps:Number;		public function DamageEffect(dps:Number)		{			super(Effect.DURATION_SINGLE, 0);			_dps = dps		}				override public function update():void{			if(_target != null){				_target.hp -= _dps * WizardsG.TIME_DIFF;			}			super.update();		}	}}